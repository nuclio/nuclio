{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "globals": {
        "$": false,
        "angular": true,
        "browser": false,
        "repeater": false,
        "element": false,
        "inject": false,
        "afterEach": false,
        "beforeEach": false,
        "confirm": false,
        "context": false,
        "describe": false,
        "expect": false,
        "it": false,
        "jasmine": false,
        "mostRecentAjaxRequest": false,
        "qq": false,
        "runs": false,
        "spyOn": false,
        "spyOnEvent": false,
        "waitsFor": false,
        "xdescribe": false,
        "L": true
    },
    "parserOptions": {
        "ecmaVersion": 6
    },
    "plugins": [
        "angular"
    ],
    "rules": {
        "angular/controller-as": 2,
        "angular/controller-as-route": 2,
        "angular/controller-name": [
            2,
            "/[A-Z].*Controller$/"
        ],
        "angular/definedundefined": 2,
        "angular/directive-name": [
            2,
            "igz"
        ],
        "angular/empty-controller": 2,
        "angular/json-functions": 2,
        "angular/interval-service": 2,
        "angular/no-cookiestore": 2,
        "angular/no-http-callback": 2,
        "angular/no-service-method": 2,
        "angular/on-watch": 2,
        "angular/prefer-component": 1,
        "angular/timeout-service": 2,
        "angular/typecheck-array": 2,
        "angular/typecheck-function": 2,

        // Possible Errors category
        "no-cond-assign": 0,
        "no-console": 0,
        "no-debugger": 0,
        "no-extra-boolean-cast": 0,
        "no-irregular-whitespace": 2,
        "no-unexpected-multiline": 0,

        // Best Practices category
        "complexity": [
            2,
            10
        ],
        "curly": [
            2,
            "all"
        ],
        "eqeqeq": 2,
        "guard-for-in": 2,
        "no-caller": 2,
        "no-eval": 2,
        "no-eq-null": 2,
        "no-extend-native": 2,
        "no-global-assign": 2,

        "no-invalid-this": 2,
        "no-iterator": 2,
        "no-loop-func": 2,
        "no-multi-str": 2,
        "no-new-wrappers": 2,
        "no-new": 2,
        "no-new-func": 2,
        "no-proto": 2,
        "no-script-url": 2,
        "wrap-iife": 2,

        // Strict category
        "strict": 0,

        // Variables category
        "no-shadow": 2,
        "no-undef": 2,
        "no-unused-vars": 0,
        "no-use-before-define": [
            2,
            {
                "functions": false
            }
        ],

        // Stylistic Issues category
        "array-bracket-spacing": [
            2,
            "never",
            {}
        ],
        "brace-style": [
            2,
            "1tbs"
        ],
        "camelcase": [
            2,
            {
                "properties": "never"
            }
        ],
        "comma-dangle": 0,
        "eol-last": 2,
        "indent": [
            1,
            4,
            {
                "SwitchCase": 1
            }
        ],
        "key-spacing": [
            2,
            {
                "afterColon": true,
                "beforeColon": false
            }
        ],
        "keyword-spacing": [
            2,
            {
                "before": true
            }
        ],
        "max-depth": [
            2,
            5
        ],
        "max-params": [
            2,
            20
        ],
        "max-statements": [
            2,
            50
        ],
        "new-cap": 2,
        "no-bitwise": 2,
        "no-multiple-empty-lines": 2,
        "no-trailing-spaces": 2,
        "operator-linebreak": [
            2,
            "after"
        ],
        "quotes": [
            2,
            "single"
        ],
        "semi": 0,
        "space-before-blocks": [
            2,
            "always"
        ],
        "space-before-function-paren": [
            2,
            {
                "anonymous": "always",
                "named": "never"
            }
        ],
        "space-in-parens": [
            2,
            "never"
        ],
        "space-infix-ops": 2,
        "space-unary-ops": [
            2,
            {
                "nonwords": false,
                "overrides": {}
            }
        ],

        // ECMAScript 6 category
        "require-yield": 0
    }
}